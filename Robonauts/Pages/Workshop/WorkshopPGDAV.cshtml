@page
@model Robonauts.Pages.Workshop.WorkshopPGDAVModel
@{
    ViewData["Title"] = "PGDAV Workshop";
    Layout = "~/Pages/_CommonLayout.cshtml";
}

@section Javascripts{
    <script src="http://code.jquery.com/jquery-3.1.1.min.js"></script>
    <style>
        .carousel-inner > .item > img,
        .carousel-inner > .item > a > img {
            width: 70%;
            margin: auto;
        }
    </style>
}


<div class="container">

    <div class="panel" style="background-color:#FFFFFF;" id="top">
        <div class="panel-heading">
            <h1 class="text-capitalize text-center" style="color:darkorange" ;>Workshops conducted by Us</h1>
        </div>
    </div>
    <div>

        <br />

        <!-- pgdav-->

        <div class="panel panel-success" id="pgdav">
            <div class="panel-heading">
                <h3 class="panel-title text-capitalize">Workshop at PGDAV College, University of Delhi</h3>
            </div>
            <div class="panel-body text-justify">
                The Centre for Robotics and Artificial Intelligence conducted workshop on <i><strong>"Introduction to Robotics"</strong></i> at PGDAV College, University of Delhi. Students were introduced to Atmega 2560 and FireBird V robots.<br />
                <br />
                <div>
                    <div class="row">
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav1.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav2.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav4.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav5.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav6.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav7.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-6 col-xs-12">
                            <img class="img-responsive" src="~/images/gallery/pgdav8.jpg" alt="PGDAV Workshop Pic" style="padding: 10px 10px 10px 10px; border:1px solid #DFF0D8;" />
                        </div>
                        <div class="col-lg-6 col-xs-12">

                        </div>
                    </div>
                </div>
            </div>
        </div>




    </div>
    <div class="row">
        <div class="col-lg-3 col-xs-12"></div>
        <div class="col-lg-offset-1 col-lg-1 col-xs-12 text-center text-capitalize">
            <a class="page-scroll btn btn-info" asp-page="AllWorkshops">See All Workshops</a>
        </div>
        <div class="col-lg-1 col-xs-12"></div>
        <div class="col-lg-1 col-xs-12 text-center text-capitalize">
            <a asp-page="Index" class="btn btn-success">Back To Workshops</a>
        </div>
        <div class="col-lg-4 col-xs-12"></div>
    </div>
</div>
